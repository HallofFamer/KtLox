var a = nil;
println("value of a: ");
println(a);
println("class of a: ");
println(a.className);
println("string value of a: ");
println(a.toString());
println("a is nil?");
println(a.isNil());
println("a is boolean? ");
println(a.isBoolean());
println("a is instance of Nil class?");
println(a.instanceOf(Nil));
println("a is instance of Object class?");
