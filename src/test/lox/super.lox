class Doughnut{
    cook(){
        print "Fry until golden brown.";
    }
}

class BostonCream < Doughnut{
    cook(){
        super.cook();
        print "Pipe full of custard and coat with chocolate.";
    }
}

BostonCream().cook();

class A{
    method(){
        print "Method A";
    }
}

class B < A{
    method(){
        print "Method B";
    }

    test(){
        super.method();
    }
}

class C < B{ }

C().test();